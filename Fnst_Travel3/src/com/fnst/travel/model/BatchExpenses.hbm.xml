<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_BatchExpenses" name="com.fnst.travel.model.BatchExpenses">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="routeId"/>
    <property name="batchId"/>
    <property name="isBoard"/>
    <property name="batchNo"/>
    <property name="batchName"/>
    <property name="routeNo"/>
    <property name="routeName"/>
    <property name="username"/>
    <property name="name"/>
    <property name="familyNum"/>
    <property name="companyExpenses"/>
    <property name="personalExpenses"/>
    <property name="familyExpenses"/>
    <property name="familyOff"/>
    <property name="totalExpenses"/>
    <property name="needExpenses"/>
    <property name="saveStatus"/>
    <property name="remarks"/>
    
  </class>
</hibernate-mapping>
