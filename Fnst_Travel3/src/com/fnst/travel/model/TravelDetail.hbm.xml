<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_TravelDetail" name="com.fnst.travel.model.TravelDetail">
    <id name="id">
      <generator class="native"/>
    </id>
    <many-to-one unique="true" name="travelRoute"/>
    <property name="details" length="2048"/>
  </class>
</hibernate-mapping>
