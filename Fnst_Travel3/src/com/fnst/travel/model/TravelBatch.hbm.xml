<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="t_TravelBatch" name="com.fnst.travel.model.TravelBatch">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="startDate"/>
    <property name="endDate"/>
    <property name="subsidy"/>
    <property name="relationBatch"/>
    <property name="status"/>
    <property name="abroadStatus"/>
    <property name="internalStatus"/>
    <property name="remarks"/>
    <property name="batchNo"/>
    <property name="name"/>
    <property name="intentionNum"/>
    <many-to-one name="travelYearth"/>
  </class>
</hibernate-mapping>
